<Window x:Class="Solsystem.MainWindow" x:Name="spaceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Solsystem"
        mc:Ignorable="d"
        SizeToContent="Manual"
        WindowState="Maximized"
        Title="MainWindow">
    <Grid Name="spaceGrid">
        <Canvas Margin="0,0,0,0" Background="Black" Name="spaceFrame"/>
        <Canvas Margin="0,0,0,0" HorizontalAlignment="Right" VerticalAlignment="Bottom" Background="DarkSlateBlue" Name="objectFrame"/>
    </Grid>
</Window>
